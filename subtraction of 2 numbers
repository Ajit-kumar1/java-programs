import java.util.Scanner;
public class Main{
    public static void main(String[] args)
    {
        Scanner sc= new Scanner(System.in);
        int a,b,c;
        System.out.println("Enter first number:");
        a=sc.nextInt();
        System.out.println("Enter second Number:");
        b=sc.nextInt();
        c=a-b;
        System.out.println("Subtraction = "+c);
    }
}
